<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="addr" value="net.tcp://192.168.0.4:4000/FileService"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="FileServiceBehavior" name="WCF파일_1대다합_서버.CFile">
        <endpoint address="" binding="netTcpBinding" bindingConfiguration="DuplexBinding"
          contract="WCF파일_1대다합_서버.IFile" />
        <host>
          <baseAddresses>
            <add baseAddress="http://192.168.0.4:7000/GetFileService" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="DuplexBinding" maxBufferPoolSize="2147483647"
          maxBufferSize="2147483647" maxConnections="100" maxReceivedMessageSize="2147483647" >
          <reliableSession enabled="true" />
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="FileServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

</configuration>